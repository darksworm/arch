    xrandr --output eDP1 --auto

XF86MonBrightness{Up,Down}
    xbacklight {+,-}5 && update_backlight

XF86Audio{Raise,Lower}Volume
    pamixer -{i,d}5 && update_volume

XF86AudioMute
    ( pamixer --get-mute && pamixer -u || pamixer -m  ) && update_volume

